triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `TR_Afiliados_BI` BEFORE INSERT ON `afiliados` FOR EACH ROW BEGIN\n	IF NEW.Activo<0 OR NEW.Activo>1 THEN\n	SIGNAL SQLSTATE \'45000\' SET MESSAGE_TEXT = \'Activo debe ser 0 o 1\';\n	END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `TR_Afiliados_AI` AFTER INSERT ON `afiliados` FOR EACH ROW BEGIN\n	INSERT INTO afiliados_log(ID_Afiliado,Contrase単a,Nombre,Telefono,Direccion,\n	Correo_Primario, Correo_Secundario,Fecha_Nacimiento, Fecha_Inicio, Fecha_Hora,\n	Activo) VALUES(NEW.ID_Afiliado, NEW.Contrase単a, NEW.Primer_Nombre || \n	NEW.Segundo_Nombre || NEW.Primer_Apellido || NEW.Segundo_Apellido, NEW.Telefono,\n	NEW.Direccion, NEW.Correo_Primario, NEW.Correo_Secundario, NEW.Fecha_Nacimiento,\n	NEW.Fecha_Inicio, NOW(), NEW.Activo);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `TR_Afiliados_AU` AFTER UPDATE ON `afiliados` FOR EACH ROW BEGIN\n	INSERT INTO afiliados_log(ID_Afiliado,Contrase単a,Nombre,Telefono,Direccion,\n	Correo_Primario, Correo_Secundario,Fecha_Nacimiento, Fecha_Inicio, Fecha_Hora,\n	Activo) VALUES(NEW.ID_Afiliado, NEW.Contrase単a, NEW.Primer_Nombre || \n	NEW.Segundo_Nombre || NEW.Primer_Apellido || NEW.Segundo_Apellido, NEW.Telefono,\n	NEW.Direccion, NEW.Correo_Primario, NEW.Correo_Secundario, NEW.Fecha_Nacimiento,\n	NEW.Fecha_Inicio, NOW(), NEW.Activo);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `TR_Afiliados_BU` BEFORE UPDATE ON `afiliados` FOR EACH ROW BEGIN\n	IF OLD.Activo = 0 THEN\n		SIGNAL SQLSTATE \'45000\' SET MESSAGE_TEXT = \'El afiliado esta inactivo\';\n	END IF;\n	IF NEW.Activo<0 OR NEW.Activo>1 THEN\n	SIGNAL SQLSTATE \'45000\' SET MESSAGE_TEXT = \'Activo debe ser 0 o 1\';\n	END IF;\nEND'
